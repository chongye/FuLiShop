<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ucai.fulishop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".FuLiShopApplication"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="福利社"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.GoodsDetailsActivity" />
        <activity android:name=".activity.BoutiqueCategroyActivity" />
        <activity android:name=".activity.CategoryChildActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.PersonActivity" />
        <activity android:name=".activity.UpdateNickActivity" />
        <activity android:name=".activity.CollectActivity"></activity>
    </application>

</manifest>